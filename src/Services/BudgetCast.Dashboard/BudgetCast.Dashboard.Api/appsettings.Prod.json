{
  "IdentityManagement": {
    "ConnectionString": "Server=identity-db,1433;Database=IdentityDb;User Id=sa;Password=Install_new!"
  },
  "BudgetCast": {
    "ConnectionString": "mongodb://budgetcast-db:27017"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ExternalIdentityProviders": {
    "UiRedirectUrl": "http://localhost:4200"
  },
  "UiLinks": {
    "Root": "http://localhost:4200",
    "ResetPassword": "http://localhost:4200/account/reset-password",
    "Login": "http://localhost:4200/account/login"
  },
  "ParentDomain": "localhost",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.AspNetCore.Identity.EntityFrameworkCore": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq",
          "apiKey": "none"
        }
      },
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "Properties": {
      "ApplicationName": "BudgetCastDashboard"
    }
  }
}